<div id="wrap">
	<div id="status"></div>
	<h2>Contacts - Sample KnockoutJs Contacts App </h2>
	
	<div id="contactsRegion">
		<ul data-bind="foreach: contactsArray">
			<li>
				<span data-bind="text: firstname"></span>
				<span data-bind="text: lastname"></span>
			</li>
		</ul>	
	</div>
	
	<div id="mainRegion">
	</div>
</div>

